StoredFile
    name Text
    contentType Text
    content ByteString
    created UTCTime "default=now()"
Company
    name Text 
    url Text
    ticker Text
    status Bool default = True
    UniqueTicker ticker status
    deriving Show Read
TimeSeries
    tsid CompanyId
    refdate UTCTime
    UniqueTS tsid refdate
    close Double
    deriving Show Read
    
