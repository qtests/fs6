StoredFile
    name Text
    contentType Text
    content ByteString
    created UTCTime "default=now()"
Company
    name Text 
    url Text
    ticker Text
    status Bool default = True
    UniqueTicker ticker status
    deriving Show Read
TimeSeries
    tsid CompanyId
    refdate UTCTime
    UniqueTS tsid refdate
    close Double
    adjclose Double
    volume Double
    deriving Show Read
Story
    hashId Int
    title Text
    link Text
    content Text Maybe
    image Text Maybe
    created UTCTime "default=now()"
    deriving Eq Show
    
    
